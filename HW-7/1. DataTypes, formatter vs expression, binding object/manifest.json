{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "leverx.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "https://services.odata.org/V2/OData/OData.svc",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "leverx.app.view.App",
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "leverx.app.view",
				"controlId": "RootApp",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "FirstPage",
					"target": [
						"firstPage"
					]
				},
				{
					"pattern": "SecondPage",
					"name": "SecondPage",
					"target": [
						"secondPage"
					]
				}
			],

			"targets": {
				"firstPage": {
					"viewName": "FirstPage",
					"viewId": "firstPage",
					"viewLevel": 1
				},

				"secondPage": {
					"viewName": "SecondPage",
					"viewId": "secondPage",
					"viewLevel": 2
				},

				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.44.19",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"odata": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "InlineRepeat",
					"useBatch": false
				}
			}
		}
	}
}